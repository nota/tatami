{
  "name": "tatami",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "10.x",
    "npm": "6.x"
  },
  "description": "tatami",
  "scripts": {
    "build": "run-s build:**",
    "build:client": "browserify --verbose -t [ babelify ] -g [ uglifyify --keep_fnames ] src/client/js/index.js -o public/index.js",
    "build:sass": "sh scripts/build-sass.sh",
    "build:assets": "sh scripts/build-assets.sh",
    "start": "./node_modules/.bin/http-server ./ -p $PORT -c-1",
    "start-dev": "./node_modules/.bin/http-server ./ -p 2500 -c-1",
    "watch": "run-p watch:**",
    "watch:client": "watchify --verbose --debug -t [ babelify ] -g [ uglifyify --keep_fnames ] -p browserify-notify src/client/js/index.js -o public/index.js",
    "watch:sass": "chokidar 'src/client/css/**/*.scss' '../../assets/stylesheets' -c 'npm run build:sass -- --source-map-embed' --initial",
    "watch:assets": "chokidar 'src/client/img/**' -c 'npm run build:assets' --initial",
    "watch:dev": "npm run start-dev"
  },
  "author": "notainc",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "browserify-notify": "^1.1.2",
    "chokidar-cli": "^1.2.0",
    "node-dev": "^3.1.3",
    "standard": "11.0.0",
    "watchify": "^3.11.0"
  },
  "dependencies": {
    "babel-register": "^6.24.1",
    "babel-cli": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-regenerator-runtime": "^6.5.0",
    "babelify": "^8.0.0",
    "bootstrap": "^3.3.7",
    "browser-info": "^1.2.0",
    "browserify": "^16.2.2",
    "browserify-shim": "^3.8.14",
    "browserify-zlib": "^0.2.0",
    "dateformat": "^3.0.3",
    "debug": "^3.1.0",
    "focus-visible": "^4.1.4",
    "font-awesome": "^4.6.3",
    "highlight.js": "^9.12.0",
    "http-server": "^0.11.1",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.3",
    "path": "^0.12.7",
    "react": "^15.6.2",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^15.6.2",
    "uglifyify": "^5.0.0",
    "what-input": "^5.1.2"
  }
}
